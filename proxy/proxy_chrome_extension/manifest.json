{
  "manifest_version": 2,

  "name": "proxy chrome extension",
  "description": "",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
     "permissions": [
       "webRequest",
"webRequestBlocking",
	"storage",
       "*://*.google.com/"
      ],
  "background": {
    "scripts": ["background.js"],
    "persistent":true
  }
}